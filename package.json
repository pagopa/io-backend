{
  "name": "italia-backend",
  "version": "2.1.0",
  "minAppVersion": "0.0.0",
  "description": "Italia app and web backend",
  "main": "index.js",
  "engines": {
    "node": "10.14.2"
  },
  "scripts": {
    "prepare": "patch-package",
    "build": "tsc",
    "build-noemit": "tsc --noEmit",
    "hot-reload": "nodemon src/server.js",
    "lint": "tslint -p . -c tslint.json -t verbose",
    "lint-autofix": "tslint -p . -c tslint.json -t verbose --fix",
    "prettify": "prettier --write \"./**/*.ts\"",
    "test": "jest -i",
    "start": "node src/server.js",
    "generate:proxy-models": "npm-run-all generate:proxy:* generate:api:*",
    "generate:proxy:api-models": "rimraf generated/backend && mkdir -p generated/backend && gen-api-models --api-spec api_backend.yaml --out-dir generated/backend",
    "generate:proxy:notification-models": "rimraf generated/notifications && mkdir -p generated/notifications && gen-api-models --api-spec api_notifications.yaml --out-dir generated/notifications",
    "generate:proxy:pagopa-models": "rimraf generated/pagopa && mkdir -p generated/pagopa && gen-api-models --api-spec api_pagopa.yaml --out-dir generated/pagopa",
    "generate:proxy:public-models": "rimraf generated/public && mkdir -p generated/public && gen-api-models --api-spec api_public.yaml --out-dir generated/public",
    "generate:api:io": "rimraf generated/io-api && mkdir -p generated/io-api && gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/io-functions/v0.66.0/api/public_api_v1.yaml --no-strict --out-dir generated/io-api --request-types --response-decoders",
    "generate:api:pagopaproxy": "rimraf generated/pagopa-proxy && mkdir -p generated/pagopa-proxy && gen-api-models --api-spec https://raw.githubusercontent.com/teamdigitale/italia-pagopa-proxy/v0.8.6/api_pagopa.yaml --no-strict --out-dir generated/pagopa-proxy --request-types --response-decoders",
    "generate:test-certs": "./scripts/generate-test-certs.sh certs",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamdigitale/italia-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamdigitale/italia-backend/issues"
  },
  "homepage": "https://github.com/teamdigitale/italia-backend#readme",
  "dependencies": {
    "apicache": "^1.4.0",
    "applicationinsights": "^1.4.2",
    "awilix": "^3.0.9",
    "azure-sb": "^0.10.6",
    "body-parser": "^1.18.3",
    "date-fns": "^1.30.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-enforces-ssl": "1.1.0",
    "fp-ts": "1.12.0",
    "helmet": "3.21.0",
    "http-graceful-shutdown": "^2.3.2",
    "io-ts": "1.8.5",
    "io-ts-types": "^0.4.7",
    "italia-ts-commons": "^5.1.9",
    "json-set-map": "^1.0.2",
    "morgan": "^1.9.1",
    "node-fetch": "^2.2.0",
    "passport": "^0.4.0",
    "passport-auth-token": "^1.0.1",
    "passport-http-bearer": "^1.0.1",
    "passport-strategy": "^1.0.0",
    "patch-package": "^5.1.1",
    "range_check": "^1.4.0",
    "redis": "^2.8.0",
    "redis-clustr": "^1.6.0",
    "request-ip": "^2.1.1",
    "spid-passport": "git://github.com/teamdigitale/spid-passport.git#v1.1.7",
    "ulid": "^2.2.2",
    "validator": "^10.8.0",
    "winston": "^3.0.0",
    "x509": "^0.3.4",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@types/apicache": "^1.2.0",
    "@types/azure-sb": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/express": "4.16.0",
    "@types/express-enforces-ssl": "^1.1.1",
    "@types/glob": "^5.0.35",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^23.3.3",
    "@types/lolex": "2.1.3",
    "@types/morgan": "^1.7.35",
    "@types/node": "10.14.2",
    "@types/node-fetch": "^2.1.2",
    "@types/passport": "^0.4.6",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-strategy": "^0.2.35",
    "@types/redis": "^2.8.13",
    "@types/request-ip": "^0.0.34",
    "@types/superagent": "^3.8.4",
    "@types/supertest": "^2.0.6",
    "@types/swagger-parser": "^4.0.2",
    "@types/validator": "^9.4.2",
    "@types/xml2js": "^0.4.5",
    "@types/xmldom": "^0.1.29",
    "danger": "^4.0.2",
    "danger-plugin-digitalcitizenship": "^0.3.1",
    "italia-tslint-rules": "^1.1.3",
    "italia-utils": "^4.0.0",
    "jest": "^23.6.0",
    "lolex": "4.2.0",
    "mock-redis-client": "^0.91.13",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "superagent": "^3.8.3",
    "supertest": "^3.3.0",
    "swagger-cli": "^2.3.1",
    "swagger-parser": "^5.0.5",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.ts"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!src/clients/api/*.{ts}",
      "!src/clients/api/models/*.{ts}",
      "!src/clients/pagopa/*.{ts}",
      "!src/clients/pagopa/models/*.{ts}",
      "!src/types/api/*.{ts}",
      "!src/**/*.d.ts"
    ]
  }
}
